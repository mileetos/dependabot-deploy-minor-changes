{
  "root": true,
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb-typescript/base",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off"
  },
  "ignorePatterns": ["node_modules/", "dist/*"]
}
